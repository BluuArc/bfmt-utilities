{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-any": true,
    "quotemark": [
      true,
      "single",
      "avoid-template",
      "avoid-escape"
    ],
    "no-magic-numbers": true,
    "no-parameter-reassignment": true,
    "await-promise": true,
    "no-async-without-await": true,
    "no-console": true,
    "no-construct": true,
    "no-duplicate-variable": true,
    "no-invalid-template-strings": true,
    "no-return-await": true,
    "no-sparse-arrays": true,
    "max-line-length": false,
    "space-before-function-paren": [
      true,
      "always"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "jsdoc-format": [
      true,
      "check-multiline-start"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "no-redundant-jsdoc": true,
    "completed-docs": true,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
      ]
    },
    "align": [
      true,
      "arguments",
      "elements",
      "members",
      "parameters",
      "statements"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-irregular-whitespace": true,
    "no-trailing-whitespace": true
  },
  "rulesDirectory": []
}
